AXA,iBAWA;AAAA;;AACA,WAAA,SAAA,GAAA,KAAA,SAAA,CAAA,GAAA,CAAA,UAAA,QAAA;AAAA,eAAA,QAAA,CAAA,IAAA,CAAA;AAAA,OAAA,CAAA;AACA,WAAA,MAAA,CACA,QADA,gBACA,QADA,GACA;AACA,QAAA,IAAA,EAAA,KAAA,IADA;AAEA,QAAA,KAAA,EAAA,KAAA,KAFA;AAGA,QAAA,MAAA,EAAA,KAAA,MAHA;AAIA,QAAA,SAAA,EAAA,KAAA;AAJA,OADA,EAOA,IAPA,CAQA,YAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,UAAA,EAAA,kBAAA,EAAA,SAAA;;AACA,QAAA,MAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OAXA,EAYA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,WAAA,GAAA,CAAA,OAAA,GAAA,MAAA,EAAA,OAAA;AACA,OAdA;AAgBA;AA7BA,GArBA;AAoDA,EAAA,OApDA,qBAoDA;AAAA;;AACA,SAAA,MAAA,CAAA,QAAA,oBAAA,eAAA,GAAA,IAAA,CACA,YAAA;AACA,MAAA,MAAA,CAAA,WAAA,GAAA,MAAA,CAAA,YAAA;AACA,KAHA,EAIA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,MAAA,WAAA,GAAA,CAAA,OAAA,GAAA,MAAA,EAAA,OAAA;AACA,KANA;AAQA;AA7DA,CAAA","sourcesContent":["<template>\n  <section class=\"page-section\">\n    <b-container>\n      <HeaderPage title=\"Adicionar Quiz\"/>\n      \n      <!--FORM-->\n      <b-row>\n        <b-col cols=\"2\"></b-col>\n        <b-col cols=\"8\">\n          <form @submit.prevent=\"add\">\n            <div class=\"form-group\">\n              <input\n                v-model=\"name\"\n                type=\"text\"\n                class=\"form-control\"\n                id=\"txtName\"\n                placeholder=\"escreve o nome\"\n                required\n              />\n            </div>\n            <div class=\"form-group\">\n              <input\n                v-model=\"level\"\n                type=\"text\"\n                onmouseenter=\"(this.type='number')\"\n                onmouseleave=\"(this.type='text')\"\n                min=\"1\"\n                max=\"10\"\n                class=\"form-control\"\n                id=\"txtLevel\"\n                placeholder=\"indica o nível do quiz\"\n                required\n              />\n            </div>\n            <div class=\"form-group\">\n              <input\n                v-model=\"points\"\n                type=\"text\"\n                 onmouseenter=\"(this.type='number')\"\n                onmouseleave=\"(this.type='text')\"\n                min=\"1\"\n                class=\"form-control\"\n                id=\"txtLevel\"\n                placeholder=\"indica os pontos a ganhar\"\n                required\n              />\n            </div>\n            <div v-if=\"questions.length\">\n              <div class=\"row\">\n                <div class=\"col-md-9\">\n                  <h4>Lista de questões</h4>\n                </div>\n                <div class=\"col-md-3\">\n                  <h4>Ações</h4>\n                </div>\n              </div>\n            </div>\n            <div v-for=\"(question, index) in questions\" :key=\"question.id\">\n              <div class=\"form-group\">\n                <div class=\"row\">\n                  <div class=\"col-md-9\">\n                    <select\n                      v-model=\"question.id\"\n                      :key=\"index\"\n                      class=\"form-control\"\n                      required\n                    >\n                      <option\n                        v-for=\"q in getQuestionsByLevel\"\n                        :key=\"q._id\"\n                        :value=\"q._id\"\n                      >{{q.question}}</option>\n                    </select>\n                  </div>\n                  <div class=\"col-md-3\">\n                    <button\n                      @click=\"removeQuestion(index)\"\n                      type=\"button\"\n                      class=\"btn btn-outline-danger mr-2\"\n                    ><i class=\"fas fa-trash\"></i> REMOVER</button>\n                  </div>\n                </div>\n              </div>\n            </div>\n            \n            <button\n              @click=\"addQuestion\"\n              type=\"button\"\n              class=\"btn btn-outline-success mr-2\"\n            ><i class=\"fas fa-plus-square\"></i> ADICIONAR QUESTÕES</button>\n            <!--<pre>{{ $data }}</pre>-->\n            <button type=\"submit\" class=\"btn btn-outline-success mr-2\"><i class=\"fas fa-save\"></i> GRAVAR QUIZ</button>\n            <router-link\n              :to=\"{name: 'listQuizzes'}\"\n              tag=\"button\"\n              class=\"btn btn-outline-danger\"\n            ><i class=\"fas fa-window-close\"></i> CANCELAR</router-link>\n          </form>\n        </b-col>\n        <b-col cols=\"2\"></b-col>\n      </b-row>\n    </b-container>\n  </section>\n</template>\n\n<script>\nimport { ADD_QUIZ } from \"@/store/quizzes/quiz.constants\";\nimport HeaderPage from \"@/components/HeaderPage.vue\"\nimport { FETCH_QUESTIONS } from \"@/store/questions/question.constants\";\nimport router from \"@/router\";\n\nimport { mapGetters } from \"vuex\";\n\nexport default {\n  name: \"AddQuiz\",\n   components: {\n    HeaderPage\n  },\n  data: () => {\n    return {\n      name: \"\",\n      level: \"\",\n      points: \"\",\n      questions: [],\n      myQuestions: []\n    };\n  },\n  computed: {\n    ...mapGetters(\"quiz\", [\"getMessage\"]),\n    ...mapGetters(\"question\", [\"getQuestions\"]),\n    getQuestionsByLevel() {\n      return this.myQuestions.filter(question => question.level == this.level);\n    }\n  },\n  methods: {\n    setPlaceHolder: index => {\n      return `insira a ${index}ª resposta`;\n    },\n    addQuestion() {\n      this.questions.push({ id: \"\" });\n    },\n    removeQuestion(index) {\n      this.questions.splice(index, 1);\n    },\n\n    add() {\n      this.questions = this.questions.map(question => question[\"id\"]);\n      this.$store\n        .dispatch(`quiz/${ADD_QUIZ}`, {\n          name: this.name,\n          level: this.level,\n          points: this.points,\n          questions: this.questions\n        })\n        .then(\n          () => {\n            this.$alert(this.getMessage, \"Quiz adicionado!\", \"success\");\n            router.push({name: 'listQuizzes'});\n          },\n          err => {\n            this.$alert(`${err.message}`, \"Erro\", \"error\");\n          }\n        );\n    }\n  },\n  created() {\n    this.$store.dispatch(`question/${FETCH_QUESTIONS}`).then(\n      () => {\n        this.myQuestions = this.getQuestions;\n      },\n      err => {\n        this.$alert(`${err.message}`, \"Erro\", \"error\");\n      }\n    );\n  }\n};\n</script>"],"sourceRoot":"src/views/quizzes"}]}